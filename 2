def f(a):
    b = ''
    c = ''
    d = False
    for i in range(len(a)-1, -1, -1):
        if a[i] == '!' and not d:
            c = '!'
            d = True
        elif a[i] == '?' and not d:
            c = '?'
            d = True
        elif a[i] != '!' and a[i] != '?':
            b += a[i]

    b = b[::-1] + c
    return b

print(f('QWERTY!!!!!'))