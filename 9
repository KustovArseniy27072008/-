def f(a):
    a = a/2
    a = int(a)
    b = 0
    c = 0
    d = 0
    for i in range (0,10**a):
        for j in range (0,10**a):
            i = str (i)
            j = str (j)
            for k in i:
                c += int(k)
            for k in j:
                d += int(k)
            if d == c:
                b += 1
            c = 0
            d = 0
    return b

print(f(10))